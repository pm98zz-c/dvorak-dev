///////////////////////////////
partial alphanumeric_keys 
xkb_symbols "dvorak-num-dev" {
    name[Group1] = "English (developer Dvorak - NUM only)";
    
    key <AB01> {[                comma,                 comma                ]};
    key <AB02> {[                3,                     3                    ]};
    key <AB03> {[                2,                     2                    ]};
    key <AB04> {[                1,                     1                    ]};
    key <AB05> {[                minus,                 minus                ]};
    key <AB06> {[                minus,                 minus                ]};
    key <AB07> {[                1,                     1                    ]};
    key <AB08> {[                2,                     2                    ]};
    key <AB09> {[                3,                     3                    ]};
    key <AB10> {[                comma,                 comma                ]};
    key <AC01> {[                period,                period               ]};
    key <AC02> {[                6,                     6                    ]};
    key <AC03> {[                5,                     5                    ]};
    key <AC04> {[                4,                     4                    ]};
    key <AC05> {[                plus,                  plus                 ]};
    key <AC06> {[                plus,                  plus                 ]};
    key <AC07> {[                4,                     4                    ]};
    key <AC08> {[                5,                     5                    ]};
    key <AC09> {[                6,                     6                    ]};
    key <AC10> {[                period,                period               ]};
    key <AC11> {[                NoSymbol,              NoSymbol             ]};
    key <AD01> {[                percent,               percent              ]};
    key <AD02> {[                9,                     9                    ]};
    key <AD03> {[                8,                     8                    ]};
    key <AD04> {[                7,                     7                    ]};
    key <AD05> {[                slash,                 division             ]};
    key <AD06> {[                slash,                 division             ]};
    key <AD07> {[                7,                     7                    ]};
    key <AD08> {[                8,                     8                    ]};
    key <AD09> {[                9,                     9                    ]};
    key <AD10> {[                percent,               percent              ]};
    key <AD11> {[                NoSymbol,              NoSymbol             ]};
    key <AD12> {[                NoSymbol,              NoSymbol             ]};
    key <AE01> {[                NoSymbol,              NoSymbol             ]};
    key <AE02> {[                NoSymbol,              NoSymbol             ]};
    key <AE03> {[                parenleft,             parenleft            ]};
    key <AE04> {[                parenright,            parenright           ]};
    key <AE05> {[                asterisk,              multiply             ]};
    key <AE06> {[                equal,                 equal                ]};
    key <AE07> {[                asterisk,              multiply             ]};
    key <AE08> {[                parenleft,             parenleft            ]};
    key <AE09> {[                parenright,            parenright           ]};
    key <AE10> {[                NoSymbol,              NoSymbol             ]};
    key <AE11> {[                NoSymbol,              NoSymbol             ]};
    key <AE12> {[                NoSymbol,              NoSymbol             ]};
    key <BKSL> {[                NoSymbol,              NoSymbol             ]};
    key <BKSP> {[                NoSymbol,              NoSymbol             ]};
    key <SPCE> {[                0,                     0                    ]};
    key <TLDE> {[                NoSymbol,              NoSymbol             ]};

};
