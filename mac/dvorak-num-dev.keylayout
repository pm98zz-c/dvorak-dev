	  <when state="caron" 		output="c" />
	  <when state="ring" 		output="o" />
	  <when state="stroke" 		output="/" />
	  <when state="breve" 		output="(" />
	  <when state="dot" 		output="." />
	  <when state="alig" 		output="a" />
	  <when state="eth" 		output="e" />
	  <when state="thorn" 		output="t" />
