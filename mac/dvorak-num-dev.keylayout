  <actions>
	<action id="compose">
	  <when state="none" 		next="compose" />
	</action>
	<action id="backquote">
	  <when state="none" 		output="`" />
	  <when state="compose" 	next="grave" />
	</action>
	<action id="apostrophe">
	  <when state="none" 		output="'" />
	  <when state="compose" 	next="acute" />
	  <when state="less"		output="&#x2018;" />
	  <when state="greater"		output="&#x2019;" />
	</action>
	<action id="caret">
	  <when state="none" 		output="^" />
