	<action id="quote">
	  <when state="none" 		output="&#x22;" />
	  <when state="compose" 	next="diaeresis" />
	  <when state="less"		output="&#x201C;" />
	  <when state="greater"		output="&#x201D;" />
	</action>
	<action id="comma">
