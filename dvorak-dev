    key <AC09> {[                n,                     N,                     ntilde,                Ntilde,                6,                     6                    ]};
    key <AC10> {[                s,                     S,                     ssharp,                U1E9E,                 period,                period               ]};
    key <AC11> {[                minus,                 underscore,            hyphen,                endash,                NoSymbol,              NoSymbol             ]};
    key <AD01> {[                semicolon,             colon,                 dead_diaeresis,        NoSymbol,              percent,               percent              ]};
    key <AD02> {[                comma,                 less,                  guillemotleft,         U201C,                 9,                     9                    ]};
