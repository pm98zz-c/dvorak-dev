    key <AD07> {[                g,                     G,                     oe,                    OE,                    7,                     7                    ]};
    key <AD08> {[                c,                     C,                     ccedilla,              Ccedilla,              8,                     8                    ]};
    key <AD09> {[                r,                     R,                     registered,            trademark,             9,                     9                    ]};
    key <AD10> {[                l,                     L,                     copyright,             numerosign,            percent,               percent              ]};
    key <AD11> {[                slash,                 question,              questiondown,          NoSymbol,              NoSymbol,              NoSymbol             ]};
    key <AD12> {[                at,                    asciicircum,           dead_circumflex,       dead_caron,            NoSymbol,              NoSymbol             ]};
    key <AE01> {[                ampersand,             percent,               NoSymbol,              NoSymbol,              NoSymbol,              NoSymbol             ]};
