    key <TLDE> {[                plus,                  asciitilde,            dead_tilde,            NoSymbol,              NoSymbol,              NoSymbol             ]};

    include "level5(modifier_mapping)"
    include "level3(ralt_switch)"
    include "level5(caps_switch)"
    include "shift(both_shiftlock)"

