    key <AE06> {[                equal,                 1,                     sterling,              NoSymbol,              equal,                 equal                ]};
    key <AE07> {[                asterisk,              0,                     NoSymbol,              NoSymbol,              asterisk,              multiply             ]};
    key <AE08> {[                exclam,                2,                     exclamdown,            U00b2,                 parenleft,             parenleft            ]};
    key <AE09> {[                parenright,            4,                     onehalf,               NoSymbol,              parenright,            parenright           ]};
