    key <AB08> {[                w,                     W,                     yacute,                Yacute,                2,                     2                    ]};
    key <AB09> {[                v,                     V,                     ydiaeresis,            Ydiaeresis,            3,                     3                    ]};
    key <AB10> {[                z,                     Z,                     U0133,                 U0132,                 comma,                 comma                ]};
    key <AC01> {[                a,                     A,                     agrave,                Agrave,                period,                period               ]};
